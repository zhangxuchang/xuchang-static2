{
    "targets"                 : {
        "http-target"         : {
            "binding-product": "xuchang-test-static",
            "type"           : "static",
            "hosts"          : "xuchang-static.oasgames.com",
            "stop_before_run": true
        },

        "run-once-cli-online" : {
            "binding-product": "run-once-cli-online",
            "type"           : "cli",
            "command"        : "echo 'run once cli' >> /tmp/deploy-center.log",
            "keepalive"      : false
        }
    },
    "log_files"               : {
        "log_handler": {
            "path"                    : "/data/logs/*/",
            "age"                     : "864000",
            "aws_group"               : "dc-log-demo",
            "aws_stream"              : "dc-log-demo",
            "datetime_format"         : "",
            "multi_line_start_pattern": "%5c%5b%5cd*%5c%5d",
            "file_fingerprint_lines"  : "1"
        }
    },
    "node_auto_build_disabled": false,
    "composer_enabled"        : false,
    "prepare_container_script": "echo 'xuchang test: prepare container script demon' >> /tmp/deploy-center.log",
    "pre_start_script"        : "echo 'xuchang test:  pre start script demon' >> /tmp/deploy-center.log"
}
