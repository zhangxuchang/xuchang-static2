{
    "targets"                 : {
        "http-target"         : {
            "binding-product": "xuchang-test",
            "type"           : "static",
            "hosts"          : "xuchang-static.oasgames.com",
            "stop_before_run": false
        },
        "run-once-cli"        : {
            "binding-product": "build-control-test",
            "type"           : "cli",
            "command"        : "echo 'run once cli' >> /tmp/deploy-center.log",
            "keepalive"      : false
        },
        "run-once-cli-online" : {
            "binding-product": "xuchang-test-static",
            "type"           : "cli",
            "command"        : "echo 'run once cli' >> /tmp/deploy-center.log",
            "keepalive"      : false
        },
        "http-target-beijing" : {
            "binding-product": "xuchang-test-beijing",
            "type"           : "static",
            "hosts"          : "xuchang-static.oasgames.com",
            "stop_before_run": false
        },
        "run-once-cli-beijing": {
            "binding-product": "xuchang-test-beijing",
            "type"           : "cli",
            "command"        : "echo 'run once cli' >> /tmp/deploy-center.log",
            "keepalive"      : false
        }
    },
    "node_sub_projects"       : [
        "slimvue-test"
    ],
    "log_files"               : {
        "log_handler": {
            "path"                    : "/data/logs/*/",
            "age"                     : "864000",
            "aws_group"               : "dc-log-demo",
            "aws_stream"              : "dc-log-demo",
            "datetime_format"         : "",
            "multi_line_start_pattern": "%5c%5b%5cd*%5c%5d",
            "file_fingerprint_lines"  : "1"
        }
    },
    "node_auto_build_disabled": true,
    "composer_enabled"        : false,
    "prepare_container_script": "echo 'run prepare_container_script' >> /tmp/deploy-center.log",
    "pre_start_script"        : "echo 'run pre_start_script' >> /tmp/deploy-center2.log"
}
